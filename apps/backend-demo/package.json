{
  "name": "@myparcel-pdk/backend-demo",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "exports": "./src/index.ts",
  "files": [
    "./src"
  ],
  "scripts": {
    "build": "nitropack build",
    "postinstall": "npx nitropack prepare",
    "lint": "run ws:lint \"$(pwd)\"",
    "lint:fix": "run ws:lint:fix \"$(pwd)\"",
    "serve": "nitropack dev"
  },
  "devDependencies": {
    "@apollo/server": "^4.4.1",
    "@myparcel-pdk/common": "workspace:*",
    "@myparcel/ts-utils": "^1.9.0",
    "@types/node": "^20.2.5",
    "h3": "^1.5.0",
    "nitropack": "^2.2.3",
    "serverless": "^3.28.1",
    "serverless-plugin-typescript": "^2.1.4",
    "typescript": "^5.0.3"
  },
  "nx": {
    "targets": {
      "build": {
        "inputs": [
          "{projectRoot}/nitro.config.ts",
          "{projectRoot}/routes/**/*",
          "{projectRoot}/src/**/*"
        ],
        "outputs": [
          "{projectRoot}/dist",
          "{projectRoot}/.netlify/functions-internal",
          "{projectRoot}/.output",
          "{projectRoot}/.nitro"
        ]
      }
    }
  }
}
