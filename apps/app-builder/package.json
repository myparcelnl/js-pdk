{
  "name": "@myparcel-pdk/app-builder",
  "version": "1.0.0-alpha.4",
  "type": "module",
  "exports": {
    ".": {
      "node": "./dist/index.js"
    },
    "./src": {
      "node": "./src/index.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "pdk-builder": "./dist/index.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "run ws:tsup:build \"$(pwd)\"",
    "build:dev": "run ws:tsup:build:dev \"$(pwd)\"",
    "clean": "run ws:clean \"$(pwd)\"",
    "lint": "run ws:lint \"$(pwd)\"",
    "start": "ts-node --project ./tsconfig.build.json src/index.ts",
    "typecheck": "run ws:tsup:typecheck \"$(pwd)\""
  },
  "devDependencies": {
    "@myparcel-pdk/build-tsup": "workspace:*",
    "@types/archiver": "^5.3.1",
    "@types/commander": "^2.12.2",
    "@types/debug": "^4.1.7",
    "@types/inquirer": "^9.0.3",
    "@types/interpret": "^1.1.1",
    "@types/liftoff": "^4.0.0",
    "archiver": "^5.3.1",
    "chalk": "^5.2.0",
    "commander": "^10.0.0",
    "debug": "^4.3.4",
    "inquirer": "^9.1.4",
    "interpret": "^3.1.1",
    "liftoff": "^4.0.0",
    "supports-color": "^9.3.1",
    "tsup": "^6.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
