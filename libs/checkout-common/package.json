{
  "name": "@myparcel-pdk/checkout-common",
  "version": "1.1.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/myparcelnl/js-pdk.git",
    "directory": "libs/checkout-common"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./testing": {
      "types": "./dist/__tests__/index.d.ts",
      "default": "./src/__tests__/index.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "./src",
    "./dist"
  ],
  "scripts": {
    "build": "run clean && vite build --config vite-testing.config.ts && vite build",
    "build:dev": "run clean && vite build --config vite-testing.config.ts --mode=development && vite build --mode=development",
    "clean": "run ws:clean \"$(pwd)\"",
    "lint": "run ws:lint \"$(pwd)\"",
    "lint:fix": "run ws:lint:fix \"$(pwd)\"",
    "test": "run ws:test \"$(pwd)\"",
    "test:coverage": "run ws:test:coverage \"$(pwd)\"",
    "test:run": "run ws:test:run \"$(pwd)\"",
    "test:update": "run ws:test:update \"$(pwd)\"",
    "typecheck": "run ws:ts:typecheck \"$(pwd)\""
  },
  "dependencies": {
    "@myparcel-pdk/common": "workspace:^1.3.1",
    "@myparcel/constants": "^2.0.0",
    "@myparcel/delivery-options": "^6.0.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@myparcel-pdk/build-vite": "workspace:*",
    "radash": "^12.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.4.10",
    "vitest": "^2.1.3"
  },
  "engines": {
    "node": ">=20"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "deployGroup": "checkout"
}
