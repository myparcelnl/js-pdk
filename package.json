{
  "name": "@myparcelnl/pdk-frontend",
  "version": "0.0.0",
  "scripts": {
    "install": "npx only-allow pnpm",
    "prepare": "husky install",
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress run --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "lint-staged": {
    "src/**/*.{ts,vue}": "eslint --fix",
    "package.json": "npx sort-package-json"
  },
  "prettier": "@myparcel/prettier-config",
  "release": {
    "extends": "@myparcel/semantic-release-config/github-npm"
  },
  "dependencies": {
    "pinia": "^2.0.14",
    "plop": "^3.1.0",
    "tailwindcss": "^3.1.3",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@myparcel/eslint-config": "^3.6.0",
    "@myparcel/prettier-config": "^1.0.0",
    "@myparcel/semantic-release-config": "^3.0.0",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.40",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/test-utils": "^2.0.1",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.7",
    "cypress": "^10.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jsdom": "^19.0.0",
    "lint-staged": "^13.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.0",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.7.3",
    "vite": "^2.9.12",
    "vitest": "^0.15.0",
    "vue-tsc": "^0.37.8"
  }
}
